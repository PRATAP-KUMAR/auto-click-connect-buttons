{
  "manifest_version": 3,
  "name": "LinkedIn Auto Connect",
  "description": "Automate the clicking on Connect button, skip on message button",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "LinkedIn Auto Connect",
    "default_icon": "custom_icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage",
    "declarativeContent"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*/"
  ]
}